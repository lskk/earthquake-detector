{
  char y[2];
  byte z[2];
  int16_t yz;
  float t;
  int len,i,j,k;

  len = x.length();
  x.toCharArray(y,len+1);

  z[1] = (int8_t) y[0];
  z[0] = (int8_t) y[1];
  
